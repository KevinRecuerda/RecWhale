  "description": "Provides react core helpers.",
  "repository": {
    "type": "git",
    "url": "https://github.com/KevinRecuerda/RecWhale.git",
    "directory": "src/recwhale-react-core"
  },
  "keywords": [
    "ts",
    "typescript",
    "extension",
    "react",
    "core",
    "helper"
  ],
  "homepage": "https://github.com/KevinRecuerda/RecWhale",
